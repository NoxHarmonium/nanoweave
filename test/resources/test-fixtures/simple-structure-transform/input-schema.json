{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "person": {
      "$id": "/properties/person",
      "type": "object",
      "properties": {
        "first_name": {
          "$id": "/properties/person/properties/first_name",
          "type": "string",
          "title": "The First_name Schema ",
          "default": "",
          "examples": [
            "John"
          ]
        },
        "middle_name": {
          "$id": "/properties/person/properties/middle_name",
          "type": "string",
          "title": "The Middle_name Schema ",
          "default": "",
          "examples": [
            "Bartholomew"
          ]
        },
        "last_name": {
          "$id": "/properties/person/properties/last_name",
          "type": "string",
          "title": "The Last_name Schema ",
          "default": "",
          "examples": [
            "Smith"
          ]
        },
        "salutation": {
          "$id": "/properties/person/properties/salutation",
          "type": "string",
          "title": "The Salutation Schema ",
          "default": "",
          "examples": [
            "Mr"
          ]
        }
      }
    },
    "address": {
      "$id": "/properties/address",
      "type": "object",
      "properties": {
        "street_name": {
          "$id": "/properties/address/properties/street_name",
          "type": "string",
          "title": "The Street_name Schema ",
          "default": "",
          "examples": [
            "Fake House, 123 Fake St"
          ]
        },
        "suburb": {
          "$id": "/properties/address/properties/suburb",
          "type": "string",
          "title": "The Suburb Schema ",
          "default": "",
          "examples": [
            "Fakington"
          ]
        },
        "state": {
          "$id": "/properties/address/properties/state",
          "type": "string",
          "title": "The State Schema ",
          "default": "",
          "examples": [
            "Fakenstien"
          ]
        },
        "postcode": {
          "$id": "/properties/address/properties/postcode",
          "type": "string",
          "title": "The Postcode Schema ",
          "default": "",
          "examples": [
            "123fake"
          ]
        },
        "country": {
          "$id": "/properties/address/properties/country",
          "type": "string",
          "title": "The Country Schema ",
          "default": "",
          "examples": [
            "Fakaria"
          ]
        }
      }
    },
    "orders": {
      "$id": "/properties/orders",
      "type": "array",
      "items": {
        "$id": "/properties/orders/items",
        "type": "object",
        "properties": {
          "order_id": {
            "$id": "/properties/orders/items/properties/order_id",
            "type": "string",
            "title": "The Order_id Schema ",
            "default": "",
            "examples": [
              "123456"
            ]
          },
          "product_id": {
            "$id": "/properties/orders/items/properties/product_id",
            "type": "string",
            "title": "The Product_id Schema ",
            "default": "",
            "examples": [
              "654321"
            ]
          },
          "quantity": {
            "$id": "/properties/orders/items/properties/quantity",
            "type": "integer",
            "title": "The Quantity Schema ",
            "default": 0,
            "examples": [
              "654321"
            ]
          },
          "total_cost": {
            "$id": "/properties/orders/items/properties/total_cost",
            "type": "string",
            "title": "The Total_cost Schema ",
            "default": "",
            "examples": [
              "4.12"
            ]
          }
        }
      }
    }
  }